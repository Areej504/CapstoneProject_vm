            "input": {
                "arg1": [
                    {
                        "time": "0",
                        "value": -100
                    }
                ],
                "arg2": [
                    {
                        "time": "3",
                        "value": -100
                    }
                ]
            },
            "expected_output": {
                "sum": [
                    {
                        "time": "5",
                        "value": -200
                    }
                ]
            }
        },
        {
            "test_case_id": "T2",
            "description": "Maximum boundary values for arg1 and arg2.",
            "input": {
                "arg1": [
                    {
                        "time": "0",
                        "value": 100
                    }
                ],
                "arg2": [
                    {
                        "time": "3",
                        "value": 100
                    }
                ]
            },
            "expected_output": {
                "sum": [
                    {
                        "time": "5",
                        "value": 200
                    }
                ]
            }
        },
        {
